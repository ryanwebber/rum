;
; Core Module Prelude
;

; def-fn
(#Call :__core$def_fn
    (#Call :__core$quote def-fn!)
    (#Call :__core$quote (name args body))
    (#Call :__core$quote (#Call :__core$def_fn name args body)))

; quote
(def-fn! quote! (a) a)

; evaluate
(def-fn! eval (expr) (#Call :__core$evaluate expr))

; set
(def-fn! set! (bindings body) (#Call :__core$set bindings body))
