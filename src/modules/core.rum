;
; Core Module Prelude
;

; def-fn
(#Call :__core$def_fn
    (#CallMacro :__core$quote def-fn!)
    (#CallMacro :__core$quote (name args body))
    (#CallMacro :__core$quote (#Call :__core$def_fn name args body)))

; quote
(def-fn! quote! (a) a)

; evaluate
(def-fn! eval (expr) (#Call :__core$evaluate expr))

; with
(def-fn! with! (bindings body) (#Call :__core$with bindings body))
